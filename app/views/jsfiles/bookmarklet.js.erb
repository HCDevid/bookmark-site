$(function (){
	var newSS, styles='#bookmarklet {background: white;border-radius: 10px;border: 1px solid;width: 200px;height: 100px;top: 10px;right: 30%;position:fixed;z-index:3;}';
	if(document.createStyleSheet) { 
		document.createStyleSheet("javascript:'"+styles+"'");
	}
	else {
		newSS=document.createElement('link'); 
		newSS.rel='stylesheet';
		newSS.href='data:text/css,'+escape(styles);
		document.getElementsByTagName("head")[0].appendChild(newSS);
	}
	$('body').first().append('<div id="bookmarklet"><p>lol</p></div>');
	//display name
	
});